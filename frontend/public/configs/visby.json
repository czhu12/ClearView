{
  "steps": [
    {
      "name": "ToCanvas",
      "params": {
        "width": 512,
        "height": 512
      }
    },
    {
      "name": "CropImage",
      "params": {
        "percentageOfX": 0.31,
        "percentageOfY": 0.45,
        "width": 50,
        "height": 50,
        "outputName": "white_crop"
      }
    },
    {
      "name": "ColorNormalizer",
      "params": {
        "samples": [
          {"inputName": "white_crop", "targetColor": [255, 255, 255]}
        ],
        "inputName": "canvas",
        "outputName": "canvas"
      }
    },
    {
      "name": "CropImage",
      "params": {
        "percentageOfX": 0,
        "percentageOfY": 0.23,
        "width": 512,
        "height": 280,
        "outputName": "device_crop"
      }
    },
    {
      "name": "CropImage",
      "params": {
        "percentageOfX": 0.1,
        "percentageOfY": 0.56,
        "width": 150,
        "height": 75,
        "outputName": "is_result_ready"
      }
    },
    {
      "name": "ResultReader",
      "params": {
        "threshold":0.99,
        "colorTarget": [128, 255, 181],
        "testType": "visby_indicator",
        "inputCanvasName": "is_result_ready",
        "outputName": "filtered_is_result_ready"
      }
    },
    {
      "name": "CropImage",
      "params": {
        "percentageOfX": 0.6,
        "percentageOfY": 0.35,
        "width": 50,
        "height": 50,
        "outputName": "is_result_valid"
      }
    },
    {
      "name": "ResultReader",
      "params": {
        "threshold": 0.996,
        "colorTarget": [145, 132, 165],
        "testType": "visby_result",
        "inputCanvasName": "is_result_valid",
        "outputName": "filtered_is_result_valid"
      }
    },
    {
      "name": "CropImage",
      "params": {
        "percentageOfX": 0.6,
        "percentageOfY": 0.45,
        "width": 50,
        "height": 30,
        "outputName": "flu_a"
      }
    },
    {
      "name": "ResultReader",
      "params": {
        "threshold": 0.996,
        "colorTarget": [145, 132, 165],
        "testType": "visby_result",
        "inputCanvasName": "flu_a",
        "outputName": "filtered_flu_a"
      }
    },
    {
      "name": "CropImage",
      "params": {
        "percentageOfX": 0.6,
        "percentageOfY": 0.5,
        "width": 50,
        "height": 30,
        "outputName": "flu_b"
      }
    },
    {
      "name": "ResultReader",
      "params": {
        "threshold": 0.996,
        "colorTarget": [145, 132, 165],
        "testType": "visby_result",
        "inputCanvasName": "flu_b",
        "outputName": "filtered_flu_b"
      }
    },
    {
      "name": "CropImage",
      "params": {
        "percentageOfX": 0.6,
        "percentageOfY": 0.57,
        "width": 50,
        "height": 30,
        "outputName": "covid_19"
      }
    },
    {
      "name": "ResultReader",
      "params": {
        "threshold": 0.996,
        "colorTarget": [145, 132, 165],
        "testType": "visby_result",
        "inputCanvasName": "covid_19",
        "outputName": "filtered_covid_19"
      }
    },
    {
      "name": "CannyEdgeDetection",
      "params": {
        "inputName": "device_crop",
        "outputName": "canny",
        "blurOutputName": "blur_score"
      }
    },
    {
      "name": "CropImage",
      "params": {
        "percentageOfX": 0.88,
        "percentageOfY": 0.37,
        "width": 10,
        "height": 10,
        "outputName": "color_check_crop"
      }
    },
    {
      "name": "CheckColor",
      "params": {
        "rgb": {"r": 125, "g": 104, "b": 138},
        "tolerance": 100,
        "outputName": "color_check",
        "inputCanvasName": "color_check_crop"
      }
    }
  ]
}
