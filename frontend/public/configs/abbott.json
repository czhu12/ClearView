{
  "steps": [
    {
      "name": "ToCanvas",
      "params": {
        "width": 512,
        "height": 512
      }
    },
    {
      "name": "CropImage",
      "params": {
        "percentageOfX": 0.13,
        "percentageOfY": 0.04,
        "width": 390,
        "height": 500,
        "outputName": "device_detection"
      }
    },
    {
      "name": "CropImage",
      "params": {
        "percentageOfX": 0.5,
        "percentageOfY": 0.6,
        "width": 50,
        "height": 50,
        "outputName": "white_crop"
      }
    },
    {
      "name": "CropImage",
      "params": {
        "percentageOfX": 0.5,
        "percentageOfY": 0.08,
        "width": 25,
        "height": 25,
        "outputName": "color_detection"
      }
    },
    {
      "name": "ColorNormalizer",
      "params": {
        "samples": [
          {"inputName": "white_crop", "targetColor": [255, 255, 255]}
        ],
        "inputName": "canvas",
        "outputName": "canvas"
      }
    },
    {
      "name": "CannyEdgeDetection",
      "params": {
        "inputName": "canvas",
        "outputName": "canny",
        "blurOutputName": "blur_score"
      }
    },
    {
      "name": "CheckColor",
      "params": {
        "rgb": {"r": 221, "g": 84, "b": 153},
        "tolerance": 100,
        "outputName": "banner_is_pink",
        "inputCanvasName": "color_detection"
      }
    },
    {
      "name": "CropImage",
      "params": {
        "percentageOfX": 0.55,
        "percentageOfY": 0.23,
        "width": 125,
        "height": 125,
        "outputName": "qr_detection"
      }
    },
    {
      "name": "CropImage",
      "params": {
        "percentageOfX": 0.37,
        "percentageOfY": 0.23,
        "width": 100,
        "height": 100,
        "outputName": "result_detection"
      }
    },
    {
      "name": "CannyEdgeDetection",
      "params": {
        "inputName": "result_detection",
        "outputName": "canny"
      }
    },
    {
      "name": "RotateCanvas",
      "params": {
        "inputName": "result_detection",
        "outputName": "rotated_result_detection",
        "rotation": 90
      }
    },
    {
      "name": "QRChecker",
      "params": {
        "inputCanvasName": "qr_detection",
        "regex": ".*",
        "outputName": "qr_check",
        "supportedFormats": ["data_matrix"]
      }
    },
    {
      "name": "LinearColorSpaceProjection",
      "params": {
        "inputCanvasName": "rotated_result_detection",
        "outputName": "color_projections"
      }
    },
    {
      "name": "ResultReader",
      "params": {
        "threshold":0.95,
        "colorTarget": [255, 100, 151],
        "testType": "standard",
        "inputCanvasName": "result_detection",
        "outputName": "filtered_result_detection"
      }
    }
  ]
}