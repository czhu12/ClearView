{
  "steps": [
    {
      "name": "ToCanvas",
      "params": {
        "width": 512,
        "height": 512
      }
    },
    {
      "name": "CropImage",
      "params": {
        "percentageOfX": 0.13,
        "percentageOfY": 0.04,
        "width": 390,
        "height": 500,
        "outputName": "device_crop"
      }
    },
    {
      "name": "CropImage",
      "params": {
        "percentageOfX": 0.5,
        "percentageOfY": 0.08,
        "width": 25,
        "height": 25,
        "outputName": "color_crop"
      }
    },
    {
      "name": "CheckColor",
      "params": {
        "rgb": {"r": 221, "g": 84, "b": 153},
        "tolerance": 32,
        "outputName": "banner_is_pink",
        "inputCanvasName": "color_crop"
      }
    },
    {
      "name": "CheckText",
      "params": {
        "words": ["Binax", "NOW", "COVID-19", "Ag", "CARD"],
        "outputName": "has_correct_text",
        "inputCanvasName": "device_crop"
      }
    },
    {
      "name": "CropImage",
      "params": {
        "percentageOfX": 0.55,
        "percentageOfY": 0.23,
        "width": 100,
        "height": 100,
        "outputName": "qr_crop"
      }
    },
    {
      "name": "QRChecker",
      "params": {
        "inputCanvasName": "qr_crop",
        "regex": ".*",
        "outputName": "qr_check",
        "supportedFormats": ["data_matrix"]
      }
    },
    {
      "name": "CropImage",
      "params": {
        "percentageOfX": 0.37,
        "percentageOfY": 0.23,
        "width": 100,
        "height": 100,
        "outputName": "result_crop"
      }
    },
    {
      "name": "ResultReader",
      "params": {
        "threshold":0.95,
        "colorTarget": [255, 100, 151],
        "inputCanvasName": "result_crop",
        "outputName": "red_filtered_result_crop",
        "testType": "abbott"
      }
    }
  ]
}